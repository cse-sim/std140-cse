# Surface Constants
exterior_coefficient = 18
long_wave_emissivity = 0

# Azimuths for windows on east and south walls
window_azimuths = [90,180]

<% for surface in surfaces %>
  SURFACE "<%= surface[:name] %>"
    sfExHcModel = INPUT
    sfInHcModel = INPUT
    sfExHcMult = <%= exterior_coefficient %>
    sfInHcMult = <%= surface[:interior_coefficient] %>
    sfExAbs = <%= surface[:exterior_absorptivity] %>
    sfInAbs = <%= surface[:interior_absorptivity] %>
    sfExEpsLW = 0
    sfInEpsLW = 0
    sfArea = <%= surface[:area] %>
    sfCon = <%= surface[:name] %>
    sfExCnd = SPECIFIEDT
    sfModel = FORWARD_DIFFERENCE
    sfExAbs = <%= ext_sol_abs %>
    sfInAbs = <%= int_sol_abs %>
    sfExEpsLW = <%= ext_lw_abs %>
    sfInEpsLW = <%= int_lw_abs %>
    sfExT = import(CaseData, <%= surface[:temp] %>)
    sfGrndRefl = 0.2
    sfInH
  <% if surface[:type] == "Wall" %>
    <% sym = :wall %>
    sfType = Wall
    sfAzm = <%= surface[:azm] %>
  <% elsif surface[:type] == "Ceiling" %>
    <% sym = :roof %>
    sfType = Ceiling
  <% elsif surface[:type] == "Floor" %>
    <% sym = :floor %>
    sfType = Floor
  <% elsif surface[:type] == "Door" %>
    <% sym = :door %>
    sfType = Wall
    sfAzm = <%= surface[:azm] %>
  <% elsif surface[:type] == "Window" %>
    <% for azimuth in window_azimuths %>
      <% sym = :window %>
      sfType = Wall
      sfAzm = <% azimuth %>
  <% end %>

