// BESTESTN.inp. Formerly BT2011.cse which was formerly QW9b4040.cse, so still has some QW9b4040 artifacts.
// used by Top level input file 600N.cse

   WfName="DRYCOLD_CSW2.csv" 		// 	current version of DRYCOLD for BESTEST. Contains Tsky using cse/czm algorithm.
ebTolHour=.001  ebTolDay=.001 ebTolMon=.001
wuDays=30
nSubSteps=30
DT=No  // No daylight savings time

// dbgPrintMask = $dayofyear == 20 || $dayofyear==190 ? 8192 : 4096
// dbgPrintMask = 4096
// dbgPrintMask = $dayofyear == 20 || $dayofyear==190 ? 8192 : 4096				//
//	  dbgPrintMask = $dayofyear == 337 ? 8192 : 0     // ASHWAT Debug for Dec 3	// bruce was using this line
// dbgPrintMask = $dayofyear == 31 ? 64 : 0;									// CZM DLL debug prin control

//   dbgPrintMask = $dayofyear <= 31 ? 4096 : 0 // sgdist print for January		//solar gain distrib
     dbgPrintMask = $dayofyear == 337 ? 8192 : 0								//ASHWAT call/return
	 elevation = 5279	//1609 m

//Definitions
// Standard orientations all of which rotate with bldgmxAZM
#define Front 0
#define Left  90
#define Back  180
#define Right 270

// -DAZM= BldgAzm on command line
//BldgAzm = AZM  // Orientation of front, Window on Back //PN changed AZM to 0 to temp. avoid Command Line input.
  BldgAzm = 0  // Orientation of front, Window on Back
//  windF= 1	//this was commented out. Why? The default is windF= 0.25. BRUCE? XXX
//  grndRefl = 0.2 // default = 0.2

 //------ MATERIALS section for lightweight case ------
 MATERIAL   "Plasterboard"    matCond=0.09243   matSpHt=0.2008   matDens=59.375	  //dp=(matCond*8hr/(matSpHt*matDens*pi))^0.5[ft]= 0.138-ft=1.7-in
 MATERIAL   "FiberQuilt"      matCond=0.02311   matSpHt=0.2008   matDens=0.75	  //dp =0.625 ft = 7.5 in
 MATERIAL   "WoodSiding"      matCond=0.08088   matSpHt=0.2151   matDens=33.125	  //dp =0.17 ft = 2.04 in
 MATERIAL   "TimberFloor"     matCond=0.08088   matSpHt=0.2868   matDens=40.625	  //dp =0.133 ft = 1.6 in
 MATERIAL   "Insulation"      matCond=0.02311   matSpHt=0        matDens=0	  //dp = infinite
 MATERIAL   "SunWallMat"      matCond=0.295     matSpHt=0.239    matDens=87.399	  //dp =0.977 ft = 11.7 in
 MATERIAL   "OpaqueWin"       matCond=1/(1/.52817-1/WinInFilm-1/WINEXF) matSpHt=0.239   matDens=87.399			//matCond formula seems ambiguous.
 //------ MATERIALS section for heavyweight case ------
 MATERIAL   "ConcreteBlock"    matCond=0.2946   matSpHt=0.239    matDens=87.5	  //dp =0.189 ft = 2.27 in
 MATERIAL   "FoamInsul"       matCond=0.02311   matSpHt=0.3346   matDens=0.625	  //dp =0.53 ft = 6.4 in
 MATERIAL   "ConcreteSlab"    matCond=0.6528    matSpHt=0.239    matDens=87.5	  //dp =0.282 ft = 3.4 in

 // ----- CONSTRUCTION section for lightweight case -----
 CONSTRUCTION opaque_window     // air-to-air U = .528
	    layer     lrThk=1.0      lrMat="OpaqueWin"

 CONSTRUCTION lw_wall		//discretization penetration depth dp added. BRUCE
            layer     lrThk=0.03937
            lrMat="Plasterboard"   //x/dp = 0.29
            layer     lrThk=0.2165
            lrMat="FiberQuilt"     //x/dp = 0.346
            layer     lrThk=0.02953
            lrMat="WoodSiding"				   //x/dp = 0.174

 CONSTRUCTION lw_floor
            layer     lrThk=0.08202
            lrMat="TimberFloor"				   //x/dp = 0.62
            layer     lrThk=3.291
            lrMat="Insulation"				   //x/dp = infinite

 CONSTRUCTION roofcon
            layer     lrThk=0.03937							   //x/dp = 0.29
                      lrMat="Plasterboard"
            layer     lrThk=0.3668							   //x/dp = 0.59
                      lrMat="FiberQuilt"
            layer     lrThk=0.06234
            		  lrMat="WoodSiding"				   //x/dp = 0.37

 CONSTRUCTION sunwall
//          layer     lrThk=(0.656-(2*.295/INFILM))
//                    lrMat="SunWallMat"
            layer     lrThk=0.656							   //x/dp = 0.671
                      lrMat="SunWallMat"

// ----- CONSTRUCTION section for heavyweight case -----

 CONSTRUCTION hw_wall
            layer     lrThk=0.3281							   //x/dp = 1.73      XXX --> split into three layers
                      lrMat="ConcreteBlock"
            layer     lrThk=(0.2018-(.02311/INFILM)+(.02311/1.461))
                      lrMat="FoamInsul"
            layer     lrThk=0.02953   lrMat="WoodSiding"				   //x/dp = 0.174

 CONSTRUCTION hw_floor
            layer     lrThk=0.2625							   //x/dp = 0.93       XXX --> split into two layers
                      lrMat="ConcreteSlab"
            layer     lrThk=3.291							   //x/dp = infinite
                      lrMat="Insulation"


// ----- CONSTRUCTION section for R 1000 high resistance test case ---

 CONSTRUCTION styro    //?
            layer     lrThk=23       lrMat="FoamInsul"					   //x/dp = 43 !?


// ----- GLAZETYPES --------------------------------------------------

 GLAZETYPE  Best_Double
            gtu=1/(1/.52817-1/WinInFilm-1/WINEXF)        //air-to air u = .52817 without films
            gtSHGC=0.787
            gtSMSO= 1
            gtSMSC= 1
            gtFMult=1
            gtPySHGC=1.908287712,1.135916854,-4.28852329,2.244318725
            gtDMSHGC=0.686/0.787
            gtDMRBSol=0.686/0.787


//----------------------------------------ZONE 1  ------------------------------------------------
Zone CASE //  CASE is BESTEST case#, initially "600"
	znArea=516.68                 //48 sq meters
	znVol = 4576.8
	znModel = CZM
	znTH = HEATTEMP
	znTD = 74		// controlls ventilation if any
	znTC = COOLTEMP
	znQMxH = 3400000   	//bestest says 1000 kW = 3.4 MBtuh ??
	znQMxC = -500000

	GAIN gnPower=INTGAIN gnFrRad=0.6  //gnMeter=GAINMETER  //gnEndUse=Lit

  SURFACE North_Wall
	  sfType=Wall
	  sfCon=lw_wall
	  sfArea=232.51
	  sfAzm=Front
	  sfExCnd=Ambient
	  sfModel = forward_difference
          sfExAbs=ABSORP
          sfInAbs=INABSORP

  SURFACE South_Wall
	  sfType=Wall
	  sfCon=lw_wall
	  #ifdef OPQWIN
             sfArea=232.51-(2*WINH*WINW)             //wall area 21.6 sq meters
          #else
             sfArea=232.51                           //wall area 21.6 sq meters
          #endif
	  sfAzm=Back
	  sfExCnd=Ambient
	  sfModel = forward_difference
          sfExAbs=ABSORP
          sfInAbs=INABSORP

	#ifdef SOUTHWIN	        //this #ifdef will enclose both windows...
