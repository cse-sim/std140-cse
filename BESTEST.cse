        rpType=UDT
        rpFreq=YEAR
        //reportcol colVal=@meter[HEATMETER].Y.dmd/3413      ColHead="Pk kW"	// Old BESTEST mthd (to get peak)
        reportcol colVal=@Top.qhPeak*0.00029304				 ColHead="Pk kW"	// XXX Dfn of qhPeak?
        reportcol colVal=@Top.qhPeak/1000				     ColHead="Pk kBtuh"

        reportcol colVal=@Top.qhPeakM                        ColHead="Pk Month"
        reportcol colVal=@Top.qhPeakD                        ColHead="Pk Day"
        reportcol colVal=@Top.qhPeakH                        ColHead="Pk Hr"

 REPORT "Peak Cool"
        rpType=UDT
        rpFreq=YEAR
        reportcol colVal=@Top.qcPeak*0.00029304				ColHead="Pk kW"
        reportcol colVal=@Top.qcPeak/1000					ColHead="Pk kBtuh"

        reportcol colVal=@Top.qcPeakM                        ColHead="Pk Month"
        reportcol colVal=@Top.qcPeakD                        ColHead="Pk Day"
        reportcol colVal=@Top.qcPeakH                        ColHead="Pk Hr"

