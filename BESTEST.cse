		wnMult=1
		wnModel=ASHWAT
		wnFMult= 1
		wnNGlz = 2
		wnUnfrc=0.5286
		wnSHGC=0.789
		wnDirtLoss=0
		wnExShd = NONE
		wnInShd = NONE

	   SHADE South_OH2
		ohDepth=OHDPTH    // overhang depth
		ohDistUp=ohU      //= 0.5-m; distance from top of window
		ohExL=1.64        //= 0.5-m; distance:left edge of win to left edge of oh  // from BESTEST
		ohExR=14.76       //= 4.5-m; distance:right edge of win to rght edge of oh

                wnVfSkyDf=(sqrt((ohU+WINH)*(ohU+WINH)+ OHDPTH*OHDPTH)-sqrt(ohU*ohU+OHDPTH*OHDPTH))/(2.*WINH)
#IF 0      //XXX
	   SGDIST
		sgSurf=Floor
		sgSide=INTERIOR
		sgFSO=1.0
#ENDIF
	#endif //SOUTHWIN

   SURFACE East_Wall
   	sfType=Wall
   	sfCon=lw_wall
   	sfArea=174.38
   	sfAzm=Left
   	sfModel = forward_difference
        sfExAbs=ABSORP
        sfInAbs=INABSORP

