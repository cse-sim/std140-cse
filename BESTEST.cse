  		wnWidth=WINW
 	 	//wnArea=WINH*WINW	// = 64.6-sf  =6-sm for 600
  		wnMult=1
  		wnModel=ASHWAT
  		wnFMult= 1		// glass area/window area
  		wnNGlz = 2
  		wnUnfrc=0.5286		// =3 W/sm-K; was 0.4
  		wnSHGC=0.789		// Isn't in USER Guide.      															// in QW2b4040
